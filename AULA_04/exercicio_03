#include<stdio.h>
#include<stdlib.h>

int main(){
    char caractere;
    int numero;

    scanf("%c%c", &caractere);
    scanf("%d", &numero);

    for (int i=0; i<numero; i++)
        printf("%c ", caractere);

    return 0;
}

