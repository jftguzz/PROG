#include<stdio.h>
#include<stdlib.h>

int main(){
    int numero=2;

    while (numero>=50 || numero<2 || numero%2==0 || numero == 23){
        scanf("%d", &numero);
    }
    printf("%d", numero);

    return 0;
}

